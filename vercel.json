{
    "version": 2,
    "name": "reminder-app",
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.js",
            "methods": ["  GET ", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers" : {
                "Access-Control-Allow-Origin" : "*"
            }
        }
    ]
}